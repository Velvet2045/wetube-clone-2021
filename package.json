{
  "name": "wetube",
  "version": "1.0.0",
  "description": "The best way to watch videos.",
  "scripts": {
    "db-start": "sudo service mongodb start",
    "db-end": "sudo service mongodb stop",
    "dev:assets": "webpack --mode=development -w",
    "dev:server restart": "kill -9 $(ps aux | grep 'snodes' | awk '{print $2}') && node ./src/app.js",
    "start": "node build/init.js",
    "build": "npm run build:server && npm run build:assets",
    "build:server": "babel src -d build",
    "build:assets": "webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Velvet2045/wetube-clone-2021.git"
  },
  "author": "Jay Pak<sleepingpaili@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Velvet2045/wetube-clone-2021/issues"
  },
  "homepage": "https://github.com/Velvet2045/wetube-clone-2021#readme",
  "dependencies": {
    "aws-sdk": "^2.1013.0",
    "axios": "^0.22.0",
    "bcrypt": "^5.0.1",
    "connect-mongo": "^4.6.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.2",
    "mongoose": "^6.0.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.3",
    "multer-s3": "^2.10.0",
    "node": "^16.10.0",
    "node-fetch": "^3.0.0",
    "nodejs": "^0.0.0",
    "pug": "^3.0.2",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.14.9",
    "@babel/preset-env": "^7.15.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "mini-css-extract-plugin": "^2.4.2",
    "nodemon": "^2.0.12",
    "sass": "^1.42.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.2.6",
    "webpack": "^5.58.0",
    "webpack-cli": "^4.9.0"
  }
}
